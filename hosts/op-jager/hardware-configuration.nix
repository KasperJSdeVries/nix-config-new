  fileSystems."/" = {
    device = "/dev/disk/by-uuid/0d24ced9-d5e2-4733-a49f-43b3ac8dd5da";
    fsType = "ext4";
  };

  fileSystems."/boot" = {
    device = "/dev/disk/by-uuid/9D87-06A6";
    fsType = "vfat";
    options = ["fmask=0077" "dmask=0077"];
  };

  swapDevices = [
    {device = "/dev/disk/by-uuid/29b5444d-8112-45c3-a0c1-d889378c6ae4";}
